{
  "name": "datacf",
  "version": "1.0.0",
  "description": "DataCF是一个数据采集框架",
  "scripts": {
    "start": "webpack-dev-server --open --config config/webpack.dev.js",
    "build": "cross-env NODE_ENV='prod' webpack --config config/webpack.prod.js",
    "build_test": "cross-env NODE_ENV='test' webpack --config config/webpack.prod.js",
    "test": "mocha --require @babel/register",
    "server": "node server/index.js",
    "standard": "standard --fix config/**/*.js src/**/*.js server/**/*.js test/**/*.js"
  },
  "standard": {
    "env": [
      "mocha",
      "browser"
    ],
    "globals": [
      "ActiveXObject"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "sideEffects": false,
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "koa": "^2.6.2",
    "koa-router": "^7.4.0",
    "ws": "^6.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "standard": "^12.0.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drafish/DataCF.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/drafish/DataCF/issues"
  },
  "homepage": "https://github.com/drafish/DataCF#readme"
}
